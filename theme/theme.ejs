       -moz-box-shadow: 0px 0px 1px 1px rgba(0, 0, 0, 0.4);
            box-shadow: 0px 0px 1px 1px rgba(0, 0, 0, 0.4);
}
.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li {
