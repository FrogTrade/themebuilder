}
.ui-theme-frog13 .background .wrap .menu-nav ul li a {
    width: auto;
    display: block;
    padding: 11px 12px;
    text-decoration: none;
    -webkit-transition: background 0.2s linear;
       -moz-transition: background 0.2s linear;
         -o-transition: background 0.2s linear;
    font-size: 26px;
}
/* This shows the navigation on hover */
.ui-theme-frog13 .background .wrap .menu-nav ul li:hover ul {
    z-index: 99999!important;
}
.ui-theme-frog13 .background .wrap .cont {
    padding: 100px 10px 0px 10px;
