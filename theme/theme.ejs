    padding: 0 0 0 0;
    border-right: 1px solid #949494;
    line-height: 35px;
}
.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li:hover:first-child a,
.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li.selected:first-child a,
.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li.parentselected:first-child a {
