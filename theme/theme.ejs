    text-shadow: -1px 1px 2px #154b66;
    filter: dropshadow(color=#154b66, offx=-1, offy=1);
    font-family: 'AmaticBold';
}

/* Navigation - This is the trickiest part of a theme;
   There are 3 levels of navigation, each can be styled by using the ul class of each level. Different levels can have different
   properties, but itâ€™s important to remember that styles cascade down from the top navigation...so beware!
----------------------------------------------- */
.ui-theme-frog13 .background .wrap .menu-nav {
    position: relative;
    min-height: 34px;
    width: 962px;
    margin: 0 auto;
    z-index: 99;
    font-family: 'AmaticBold';
}
.ui-theme-frog13 .background .wrap .menu-nav ul {
    margin: 0;
    padding: 0;
    position: absolute;
    list-style: none;
    clear: both;
}
/* Top level navigation */
.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 {
    width: 962px;
    z-index: 99;
    border-radius: 5px 5px 0 0;
    background: #008fc6;
    background: -moz-linear-gradient(top, #008fc6 0%, #175170 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #008fc6), color-stop(100%, #175170));
