.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li .menu-level-2 li .menu-level-3 li:hover > a,
.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li .menu-level-2 li .menu-level-3 li.selected > a,
.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li .menu-level-2 li .menu-level-3 li.parentselected > a {
