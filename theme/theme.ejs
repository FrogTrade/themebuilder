.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li .menu-level-2 li:hover > a, .ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li .menu-level-2 li.selected > a, .ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li .menu-level-2 li.parentselected > a {
    background: #154b66;
    color: white;
    border-radius: 0 0 0 0;
}
.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li .menu-level-2 li:first-child a {
    border-radius: 0 0 0 0;
}
.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li .menu-level-2 li.sites-add-page a,
.ui-theme-frog13 .background .wrap .menu-nav ul.menu-level-1 li .menu-level-2 li.sites-add-page:hover a,
