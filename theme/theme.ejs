/* Theme font; please use this format if you plan to have custom fonts as it's cross browser compatible,
   we dont allow linked fonts, they must be embedded in this way */
@font-face {
    font-family: 'AmaticBold';
    src: url('<%=DIR%>Amatic-Bold-webfont.eot');
    src: url('<%=DIR%>Amatic-Bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('<%=DIR%>Amatic-Bold-webfont.woff') format('woff'),
         url('<%=DIR%>Amatic-Bold-webfont.ttf') format('truetype'),
         url('<%=DIR%>Amatic-Bold-webfont.svg#AmaticBold') format('svg');
    font-weight: normal;
    font-style: normal;
}
.ui-theme-frog13 .background {
    width: 100%;
    height: 100%;
    min-height: 500px;
    background: url('<%=DIR%>background-btm.png') 50% 100% no-repeat;
