}
.ui-theme-frog13 .background .wrap .head h1 {
    color: white;
    font-size: 90px;
    font-weight: normal;
    letter-spacing: -1px;
    line-height: 1;
    margin: 0;
    padding: 10px;
    text-align: left;
    word-wrap: break-word; /* this stops really long words breaking the layout by wraping the content onto a second line */
    float: left;
    width: 540px;
    text-shadow: -1px 1px 2px #154b66;
    filter: dropshadow(color=#154b66, offx=-1, offy=1);
    font-family: 'AmaticBold';
}
.ui-theme-frog13 .background .wrap .head p {
